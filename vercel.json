{
    "version": 2,
    "builds": [
        {
            "src": "api/index.ts",
            "use": "@vercel/node"
        },
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist/public",
                "installCommand": "npm install",
                "buildCommand": "npm run build"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/index.ts"
        },
        {
            "src": "/objects/(.*)",
            "dest": "api/index.ts"
        },
        {
            "src": "/(.*)",
            "dest": "dist/public/$1",
            "check": true
        },
        {
            "src": "/(.*)",
            "status": 200,
            "dest": "dist/public/index.html"
        }
    ],
    "env": {
        "VERCEL": "1",
        "NODE_ENV": "production"
    }
}