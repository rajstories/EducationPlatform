{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "dist/public",
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api"
        },
        {
            "source": "/objects/:path*",
            "destination": "/api"
        },
        {
            "source": "/:path*",
            "destination": "/index.html"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}